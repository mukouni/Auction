        }

        .main .other .hongkong li {
            width: 23.9%;
            text-indent: 12px;
            line-height: 38px;
            font-size: 12px
        }

        .main .other .hongkong input {
            background-color: #000
        }

        .main .other .calendar {
            width: 137px;
            position: relative
        }

        input[type=date]::-webkit-inner-spin-button {
            visibility: hidden;
        }

        .main .other .calendar .shipmentTime {
            width: 127px;
            height: 36px;
            border: 1px solid #e9e9e9;
            display: block;
            background: #fff;
            line-height: 38px;
            font-size: 12px;
            color: #aaa;
            text-align: center
        }

        .main .other .calendar .shipmentTime.active,
        .main .other .calendar .shipmentTime.active:hover {
            color: #1054ff
        }

        .main .other .calendar .shipmentTime:hover {
            background: #e7eeff;
            color: #aaa
        }

        .main .other .kalendar {
            position: absolute;
            top: 45px;
            width: 130px;
            right: 6px;
            border: 1px solid #e9e9e9;
            z-index: 9999;
            background: #fff;
            display: block;
            color: #333
        }

        .main .other .kalendar .tli {
            font-size: 14px;
            color: #1054ff;
            position: relative
        }

        .main .other .kalendar .tli img {
            position: absolute;
            top: -12px;
            right: 63px
        }

        .main .other .kalendar ul {
            width: 100%
        }

        .main .other .kalendar ul li {
            float: left
        }

        .main .other .kalendar .year_type {
            width: 100%;
            margin: 0 auto;
            overflow: hidden
        }

        .main .other .kalendar .year_type .active a {
            border: 1px solid #1054ff;
            color: #1054ff
        }

        .main .other .kalendar .year_type li {
            float: left
        }

        .main .other .kalendar .year_type li a {
            display: block;
            text-align: center;
            height: 36px;
            line-height: 36px;
            border: 1px solid #eee;
            -webkit-box-sizing: border-box;
            box-sizing: border-box
        }

        .main .other .kalendar .months {
            overflow: hidden
        }

        .main .other .kalendar .months ul {
            overflow: hidden;
            width: 100%;
            display: none
        }

        .main .other .kalendar .months ul li {
            float: left
        }

        .main .other .kalendar .months ul li a {
            text-align: center;
            line-height: 36px;
            border: 1px solid #eee;
            height: 36px;
            display: block;
            -webkit-box-sizing: border-box;
            box-sizing: border-box
        }

        .main .other .kalendar .months ul li a:hover {
            color: #1054ff
        }

        .main .other .kalendar .months ul li .active {
            border: 1px solid #1054ff;
            color: #1054ff
        }

        .main .other .kalendar .months ul li .active a {
            color: #333
        }

        .main .other .kalendar .months .actived {
            display: block
        }

        .main .time_ranage {
            overflow: hidden
        }

        .main .time_ranage li {
            float: left;
            width: 33.33333%
        }

        .main .time_ranage li a {
            text-align: center;
            height: 36px;
            line-height: 36px;
            display: block;
            border: 1px solid #eee;
            -webkit-box-sizing: border-box;
            box-sizing: border-box
        }

        .main .time_ranage li a:hover {
            color: #1054ff
        }

        .main .time_ranage li a p {
            font-size: 12px
        }

        .main .time_ranage li .active {
            border: 1px solid #1054ff;
            color: #1054ff
        }

        .main .sure_kalendar {
            display: block;
            background: #1054ff;
            text-align: center;
            line-height: 36px;
            height: 36px;
            color: #fff;
            font-size: 14px
        }

        .main .start_departure_list {
            border: 1px solid #e9e9e9;
            /* width: 196px; */
            position: absolute;
            left: 86px;
            z-index: 999;
            top: 54px;
            -webkit-box-shadow: 0 8px 8px 0 rgba(0, 0, 0, .04);
            box-shadow: 0 8px 8px 0 rgba(0, 0, 0, .04)
        }

        .main .start_departure_list ul {
            height: 324px;
            width: 220px;
            background-color: #fff;
            height: 288px;
            overflow-y: scroll;
            overflow-x: hidden
        }

        .main .start_departure_list p {
            background: #1951fc;
            z-index: 999;
            /* width: 196px; */
            height: 36px;
            line-height: 36px;
            text-indent: 16px;
            font-size: 14px;
            color: #fff;
            position: relative
        }

        .main .start_departure_list p img {
            position: absolute;
            top: -6px;
            left: 82px
        }

        .main .start_departure_list li {
            width: 100%;
            height: 36px;
            position: relative;
            background-color: #fff;
            font-family: PingFangSC-Regular !important
        }

        .main .start_departure_list li input {
            width: 100%;
            border: 0;
            text-indent: 16px;
            height: 36px;
            line-height: 36px
        }

        .main .start_departure_list li i {
            position: absolute;
            font-size: 16px;
            top: 16px;
            right: 16px;
            cursor: pointer;
            color: #333
        }

        .main .start_departure_list li i:hover {
            color: #666
        }

        .main .start_departure_list li a {
            display: block;
            line-height: 36px;
            font-size: 12px;
            text-align: left;
            text-indent: 19px;
            color: #333
        }

        .main .start_departure_list li a.active {
            color: #1054ff
        }

        .main .start_departure_list li a.disable {
            color: #bbb
        }

        .main .errortip {
            background: #ffd5d5
        }

        .main .address {
            width: 390px;
            padding-left: 370px;
            padding-top: 10px;
            padding-bottom: 0
        }

        .main .address li {
            font-size: 12px;
            line-height: 24px;
            text-indent: 12px
        }

        .main .address li:first-child {
            margin-bottom: 10px
        }

        .main .address li input {
            width: 302px;
            height: 22px;
            border: 1px solid #e9e9e9;
            margin-left: 5px
        }

        .main .address li input:focus {
            border: 1px solid #1054ff
        }

        .main .Match_box_list .list_item .Cabinet_Size .errors input {
            border: 1px solid #ffd5d5;
            background: #ffd5d5
        }

        .main .Match_box_list .list_item .Cabinet_Size .errors input:hover {
            border: 1px solid #e7eeff
        }

        .main .Match_box_list .list_item .Cabinet_Size .errors input:focus {
            border: 1px solid #1054ff
        }

        .main .Match_box_list .list_item .Cabinet_Size .Company.errors {
            background: #ffd5d5
        }

        .main .address li .errors,
        .main .Match_box_list .list_item .Cabinet_Weight.errors input,
        .main .other .calendar .errortime {
            border: 1px solid #ffd5d5;
            background: #ffd5d5
        }

        .main .Match_box_list .list_item .Cabinet_Weight.errors input:hover {
            border: 1px solid #e7eeff
        }

        .main .Match_box_list .list_item .Cabinet_Weight.errors input:focus {
            border: 1px solid #1054ff
        }

        .main .Match_box_list .Cabinet_Weight .Company.errors {
            background: #ffd5d5
        }

        .main .Match_box_list .list_item .Cabinet_Count input:hover,
        .main .Match_box_list .list_item .Cabinet_Size input:hover,
        .main .Match_box_list .list_item .Cabinet_Weight input:hover {
            background: #e7eeff
        }

        .main .errors_pod,
        .main .errors_pol {
            background: #ffd5d5
        }
    </style>
</head>

<body cz-shortcut-listen="true">
    <div class="main">
        <div class="select_departure">
            <ul class="clearfix">
                <li>
                    <input value="@Model.StartDeparture" class="start_departure placeholder" readonly="readonly" name="StartDeparture">
                </li>
                <li class="to_departure">
                    <a>
                        <img
                            src="data:image/png;base64,">
                    </a>
                </li>
                <li>
                    <input value="@Model.EndDeparture" type="text" class="end_departure placeholder" readonly="readonly" name="EndDeparture">
                </li>
            </ul>
        </div>
        <div class="clearfix Match_box">
            <!-- <a class="pull-right">配箱指导</a> -->
        </div>
        <ul class="Match_box_list">
            <li class="clearfix list_item_title">
                <div class="pull-left Cabinet_Type">
                    <p class="title text-center">柜型</p>
                </div>
                <div class="pull-left Cabinet_Count">
                    <p class="title text-center">柜量</p>
                </div>
                <div class="pull-left Cabinet_Size">
                    <p class="title text-center">尺寸</p>
                </div>
                <div class="pull-left Cabinet_Weight">
                    <p class="title text-center">货重</p>
                </div>
            </li>
            @foreach(var item in @Model.Cabinets){
            <li class="clearfix list_item">
                <!-- <div class="pull-left plus"></div> -->

                <div class="pull-left Cabinet_Type">
                    <ul class="clearfix">
                        <li class="pull-left" @if(item.CabinetType == "20'OT,小开顶"){ @Html.DisplayName("active") }>
                            <p>20'OT</p>
                            <p>小开顶</p>
                        </li>
                        <li class="pull-left" @if(item.CabinetType == "40'OT,大开顶"){ @Html.DisplayName("active") }>
                            <p>40'OT</p>
                            <p>大开顶</p>
                        </li>
                        <li class="pull-left" @if(item.CabinetType == "20'FR,小框架"){ @Html.DisplayName("active") }>
                            <p>20'FR</p>
                            <p>小框架</p>
                        </li>
                        <li class="pull-left" @if(item.CabinetType == "40'FR,大框架"){ @Html.DisplayName("active") }>
                            <p>40'FR</p>
                            <p>大框架</p>
                        </li>
                    </ul>
                </div>
                <div class="pull-left Cabinet_Count">
                    <input value="@item.CabinetCount" max="99999" min="0" type="text" readonly="readonly">
                </div>
                <div class="pull-left Cabinet_Size clearfix">
                    <div class="pull-left Cabinet_item clearfix">
                        <input value="@item.CabinetLength" type="number" max="99999" min="0" class="Cabinet_Length placeholder" name="CabinetLength"
                            placeholder="装柜长度" readonly="readonly">
                        <div class="Company">
                            <p class="unit">
                                <span class="unit">@item.CabinetLengthUnit</span>
                            </p>
                        </div>
                    </div>
                    <div class="pull-left Cabinet_item clearfix">
                        <input value="@item.CabinetWidth" type="number" max="99999" min="0" class="Cabinet_Width placeholder" name="CabinetWidth"
                            placeholder="装柜宽度" readonly="readonly">
                        <div class="Company">
                            <p class="unit">
                                <span class="unit">@item.CabinetWidthUnit</span>
                            </p>
                        </div>
                    </div>
                    <div class="pull-left Cabinet_item clearfix">
                        <input value="@item.CabinetHeight" type="number" max="99999" min="0" class="Cabinet_Height placeholder" name="CabinetHeight"
                            placeholder="装柜高度" readonly="readonly">
                        <div class="Company">
                            <p class="unit">
                                <span class="unit">@item.CabinetHeightUnit</span>
                            </p>
                        </div>
                    </div>
                </div>
                <div class="pull-left Cabinet_Weight clearfix">
                    <input value="@item.CabinetWeight" type="number" max="99999" min="0" name="CabinetWeight" class="pull-left" placeholder="单柜货重"
                        readonly="readonly">

                    <div class="pull-right Company">
                        <p class="unit">
                            <span class="unit">@item.CabinetWeightUnit</span>
                        </p>
                    </div>
                </div>
            </li>
            }
        </ul>
        <div class="other">
            <div class="title clearfix">
                <p class="pull-left">还有普通货柜？</p>
                <p class="pull-left">还需港前服务？</p>
                <p class="pull-left">出运时间</p>
            </div>
            <div class="other_content clearfix">
                <div class="normal_Cabinet pull-left">
                    <ul class="clearfix">
                        <li class="clearfix">
                            <input name="@Model.Gp20" type="number" max="99999" min="0" class="placeholder" name="gp20"
                                readonly="readonly">
                            <p>x 20'GP</p>
                        </li>
                        <li class="clearfix">
                            <input name="@Model.Gp40" type="number" max="99999" min="0" class="placeholder" name="gp40"
                                readonly="readonly">
                            <p>x 40'GP</p>
                        </li>
                        <li class="clearfix">
                            <input name="@Model.Hc40" type="number" max="99999" min="0" class="placeholder" name="hc40"
                                readonly="readonly">
                            <p>x 40'HC</p>
                        </li>
                    </ul>
                </div>
                <div class="hongkong pull-left">
                    <ul class="list-inline">
                        <li>
                            <label>
                                <input type="checkbox" name="MotorcycleFactory" value="true" onclick="return false;" @if(Model.MotorcycleFactory){@Html.DisplayName("checked")}>
                                到厂拖车
                            </label>
                        </li>
                        <li>
                            <label>
                                <input type="checkbox" name="WarehouseLoading" value="true" onclick="return false;" @if(Model.WarehouseLoading){@Html.DisplayName("checked")}>
                                仓库装柜
                            </label>
                        </li>
                        <li>
                            <label>
                                <input type="checkbox" name="HomeLashing" value="true" onclick="return false;" @if(Model.HomeLashing){@Html.DisplayName("checked")}>
                                上门绑扎
                            </label>
                        </li>
                        <li>
                            <label>
                                @Html.DisplayFor(m => m.CustomsDeclaration)
                                <input type="checkbox" name="CustomsDeclaration" value="true" onclick="return false;" @if(Model.CustomsDeclaration){@Html.DisplayName("checked")}>
                                代理报关
                            </label>
                        </li>
                    </ul>
                </div>
                <div class="calendar pull-right">
                    <!-- <a class="time" class="shipmentTime">请选择时间</a> -->
                    <input value="@Model.ShipmentTime" type="text" name="ShipmentTime" class="shipmentTime">
                </div>
            </div>

        </div>
        <form class="form form-inline">
            <div class="form-group-inline mb-3">
                <label for="">姓名:</label>
                <div class="form-group">
                    <input value="@Model.UserName" type="text" class="placeholder" autocomplete="off" name="name" required="required"
                        readonly="readonly">
                    <div class="message none"></div>
                </div>
            </div>
            <div class="form-group-inline">
                <label for="">手机号:</label>
                <div class="form-group">
                    <input value="@Model.Tel" type="tel" class="placeholder" autocomplete="off" name="tel" required="required"
                        readonly="readonly">
                    <div class="message none"></div>
                </div>
            </div>
            <div class="form-group-inline">
                <label for="">邮箱:</label>
                <div class="form-group">
                    <input value="@Model.Email" type="email" class="placeholder" autocomplete="off" name="email" required="required"
                        readonly="readonly">
                    <div class="message"></div>
                </div>
            </div>
        </form>

    </div>
</body>

</html>