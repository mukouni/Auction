@using System.Web.Optimization
@using X.PagedList
@using X.PagedList.Mvc
@using X.PagedList.Mvc.Core
@using X.PagedList.Mvc.Common
@model SearchEquipmentViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "_ManageLayout";
}


    @await Html.PartialAsync("_IndexBodyPartial", Model,
                            new ViewDataDictionary(ViewData)
                            {})
    
@section script {
    <script>
        $( document ).ready(function() {
            $(".search .nav-link[href='/equipment/index']").addClass("active");
            $("header .nav-link[aria-controls='manage']").addClass("active");
        })
        function deleteItem(form) {
            $(form).closest("tr").find(".isDeleted").text("Yes");
        }
        function onSearchEquipmentSuccess(){
            $('[data-toggle="table"]').bootstrapTable();
        }
        function onSearchEquipmentFailure(){
            $('[data-toggle="table"]').bootstrapTable();
        }
    </script>
}